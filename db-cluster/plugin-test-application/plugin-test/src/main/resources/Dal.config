<dal name="daltest">
	<databaseSets>
		<databaseSet name="daltestdb_w" provider="mySqlProvider">
                    <add name="daltestdb_w" databaseType="Master" sharding="" connectionString="daltestdb_w"/>
        </databaseSet>
	</databaseSets>
	<LogListener>
		<logger>com.ctrip.platform.dal.sql.logging.CtripDalLogger</logger>
	</LogListener>
	<ConnectionLocator>
		<settings>
			<dataSourceConfigureProvider>com.ctrip.datasource.titan.TitanProvider</dataSourceConfigureProvider>
		</settings>
	</ConnectionLocator>
	<TaskFactory>
		<factory>com.ctrip.platform.dal.dao.CtripTaskFactory</factory>
	</TaskFactory>
</dal>
