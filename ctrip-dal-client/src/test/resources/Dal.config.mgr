<dal name="testByQA">
    <databaseSets>
        <!-- mgr 模式-->
        <databaseSet name="kevin" provider="mySqlProvider">
            <add name="kevin" databaseType="Master" sharding="" connectionString="kevin"/>
        </databaseSet>
        <!-- 单点 -->
        <databaseSet name="qconfig" provider="mySqlProvider">
                    <add name="qconfig" databaseType="Master" sharding="" connectionString="qconfig"/>
        </databaseSet>
    </databaseSets>
    <LogListener>
        <logger>com.ctrip.platform.dal.dao.CtripDalLogger</logger>
    </LogListener>
    <ConnectionLocator>
        <settings>
            <dataSourceConfigureProvider>com.ctrip.platform.dal.dao.MockCtripVariableDataSourceConfigureProvider</dataSourceConfigureProvider>
        </settings>
    </ConnectionLocator>
    <TaskFactory>
        <factory>com.ctrip.platform.dal.dao.CtripTaskFactory</factory>
    </TaskFactory>
</dal>