<dal name="testByQA">
    <databaseSets>
        <databaseSet name="testConnectionString" provider="mySqlProvider">
            <!-- mgr 模式-->
            <add name="kevin" databaseType="Master" sharding="" connectionStringProvider="com.ctrip.platform.dal.dao.MockConnectionStringConfigureProvider" connectionString="kevin"/>
            <!-- titankey -->
            <add name="dao_test" databaseType="Master" sharding="" connectionString="fxdaltestdb_w"/>
        </databaseSet>

        <databaseSet name="qconfig" provider="mySqlProvider">
            <!-- 单点 -->
            <add name="qconfig" databaseType="Master" sharding="" connectionStringProvider="com.ctrip.datasource.configure.MysqlApiConnectionStringConfigureProvider" connectionString="qconfig"/>
        </databaseSet>
    </databaseSets>
    <LogListener>
        <logger>com.ctrip.platform.dal.sql.logging.CtripDalLogger</logger>
    </LogListener>
    <ConnectionLocator>
        <settings>
            <dataSourceConfigureProvider>com.ctrip.datasource.titan.TitanProvider</dataSourceConfigureProvider>
        </settings>
    </ConnectionLocator>
    <TaskFactory>
        <factory>com.ctrip.platform.dal.dao.CtripTaskFactory</factory>
    </TaskFactory>
</dal>