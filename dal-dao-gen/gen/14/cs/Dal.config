<dal name="dal.prize.test">
  <databaseSets>
    <databaseSet name="AssembleDB" provider="sqlProvider">
      <add  name="AssembleDB_M" databaseType="Master" sharding="" connectionString="请将这段字符串替换为All-In-One中的name或者Database.config中的name"/>
    </databaseSet>
    <databaseSet name="dao_test" provider="mySqlProvider">
      <add  name="dao_test_M" databaseType="Master" sharding="" connectionString="请将这段字符串替换为All-In-One中的name或者Database.config中的name"/>
    </databaseSet>
    <databaseSet name="PerformanceTest" provider="sqlProvider">
      <add  name="PerformanceTest_M" databaseType="Master" sharding="" connectionString="请将这段字符串替换为All-In-One中的name或者Database.config中的name"/>
    </databaseSet>
  </databaseSets>

  <databaseProviders>
    <add name="sqlProvider" type="Arch.Data.DbEngine.Providers.SqlDatabaseProvider,Arch.Data"/>
    <add name="mySqlProvider" type="Arch.Data.MySqlProvider.MySqlDatabaseProvider,Arch.Data.MySqlProvider"/>
  </databaseProviders>

  <logListeners>
    <add name="clog" type="Arch.Data.Common.Logging.Listeners.CentralLoggingListener,Arch.Data" level="Information" setting=""/>
    <add name="textfile" type="Arch.Data.Common.Logging.Listeners.TextFileListener,Arch.Data" level="Information" setting="FileSize=4;FilePath=D:\log;FileName={0:yyyy_MM_dd_HH_mm_ss}.log;"/>
  </logListeners>

  <metrics name="centrallogging"/>

</dal>
